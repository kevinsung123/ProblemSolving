tmlspecialchars=true;if(useHtmlspecialchars)string=htmlspecialchars(string);var color;if(type.substr(0,7)=="keyword"){if(!stylePackage["keywords"][type.substr(7)]){color=stylePackage["normal"];type="normal"}else color=stylePackage["keywords"][type.substr(7)]}else color=stylePackage[type];if(color==undefined){throw"Color is undefined";color=stylePackage["normal"]}var preTag;var subTag;switch(returnMode){case ColorScripter.RETURN_FONT_MODE:preTag="<font color=\""+color+"\">";subTag="</font>";break;case ColorScripter.RETURN_SPAN_MODE:preTag="<span style=\"color:"+color+"\">";subTag="</span>";break;case ColorScripter.RETURN_SPAN_CSS_MODE:preTag="<span class=\"cs-"+stylePackage.packageName+"-"+languageTypes[0]+"-"+type+"\">";subTag="</span>";break}if(string.indexOf("\n")!=-1)return preTag+string.split("\n").join(subTag+"\n"+preTag)+subTag;else return preTag+string+subTag}function insertRestCode(){var tmp=false;for(var j=0;j<keywords.length;j++){if(keywords[j].indexOf(restCode)!=-1||(caseSensitive===false&&keywords[j].indexOf(restCode.toLowerCase())!=-1)){highlightedCode+=stylefy("keyword"+j,restCode);tmp=true}};if(tmp==false)highlightedCode+=htmlspecialchars(restCode);restCode=""}function htmlspecialchars(text){var o=text.split("&").join("&amp;").split("<").join("&lt;").split(">").join("&gt;").split(" ").join("&nbsp;");if(convertingTabToSpace)o=o.split("	").join("&nbsp;&nbsp;&nbsp;&nbsp;");return o}function isNumber(n){if(n!=n.trim())return false;if(n=="true"||n=="false")return true;return!isNaN(parseFloat(n))&&isFinite(n)}function procEndOfHighlightedCode(){if(highlightedCode.substr(highlightedCode.length-13,13)=="&nbsp;</span>")highlightedCode=highlightedCode.substr(0,highlightedCode.length-13)+"</span>";else if(highlightedCode.substr(highlightedCode.length-13,13)=="&nbsp;</font>")highlightedCode=highlightedCode.substr(0,highlightedCode.length-13)+"</font>";else if(highlightedCode.substr(highlightedCode.length-8,8)==" </span>")highlightedCode=highlightedCode.substr(0,highlightedCode.length-8)+"</span>";else if(highlightedCode.substr(highlightedCode.length-8,8)==" </font>")highlightedCode=highlightedCode.substr(0,highlightedCode.length-8)+"</font>";else if(highlightedCode.substr(highlightedCode.length-6,6)=="&nbsp;")highlightedCode=highlightedCode.substr(0,highlightedCode.length-6);else if(highlightedCode.substr(highlightedCode.length-1,1)==" ")highlightedCode=highlightedCode.substr(0,highlightedCode.length-1)}for(var i=0;i<codeString.length;i++){if(endString&&(i+endString.length<=codeString.length&&codeString.substr(i,endString.length)==endString)){insertRestCode();return[highlightedCode,i]}if(languageTypes.indexOf("sigil")!=-1){if(this.identifier.indexOf(codeString[i])!=-1){if(restCode[0]=="$"){highlightedCode+=stylefy("keyword0",restCode)+htmlspecialchars(codeString[i]);restCode="";continue}}}if(languageTypes.indexOf("bash")!=-1){if(i==0&&codeString.indexOf("#!/bin/bash")==0){highlightedCode+=stylefy("keyword3","#!/bin/bash");i=10;continue}}if(languageTypes.indexOf("markdown")!=-1&&!inTag){var endOfLineIdx=codeString.indexOf("\n",i+1);if(endOfLineIdx==-1)endOfLineIdx=codeString.length;if(i==0||codeString[i-1]=="\n"){if(codeString.length>=i+3&&(codeString.substr(i,3)=='---')||codeString.substr(i,3)=='___'){for(var j=i+3;true;j++)if(j==codeString.length-1||codeString[j]!=codeString[i])break;insertRestCode();highlightedCode+=stylefy("comment",codeString.substring(i,j));i=j-1;continue}if(codeString[i]=="-"||codeString[i]=="+"||codeString[i]=="*"){insertRestCode();highlightedCode+=stylefy("keyword1",codeString[i]);continue}if(isNumber(codeString[i])&&codeString.length>=i+1){var tmpIdx=codeString.indexOf(".",i+1);if(tmpIdx!=-1){insertRestCode();highlightedCode+=stylefy("keyword1",codeString.substring(i,tmpIdx+1));i=tmpIdx;continue}}if(codeString[i]=="#"){insertRestCode();highlightedCode+=stylefy("keyword0",codeString.substring(i,endOfLineIdx));i=endOfLineIdx-1;continue}if(codeString.length>=i+3&&codeString.substr(i,3)=="```"){insertRestCode();highlightedCode+=stylefy("keyword1",codeString.substring(i,endOfLineIdx));i=endOfLineIdx-1;continue}}if(codeString[i]=="["){var idx2=codeString.indexOf("]",i+1);var idx3=codeString.indexOf(")",idx2);if(idx2!=-1&&idx3!=-1&&codeString[idx2+1]=="("){insertRestCode();highlightedCode+="["+stylefy("string",codeString.substring(i+1,idx2))+"]";highlightedCode+="("+stylefy("comment",codeString.substring(idx2+2,idx3))+")";i=idx3;continue}}if(codeString[i]=="`"&&codeString.indexOf("`",i+1)!=-1){insertRestCode();highlightedCode+=stylefy("keyword1",codeString.substring(i,codeString.indexOf("`",i+1)+1));i=codeString.indexOf("`",i+1);continue}if(codeString[i]=="*"||codeString[i]=="_"){insertRestCode();highlightedCode+=stylefy("keyword1",codeString[i]);continue}}if(languageTypes.indexOf("php")!=-1){if(this.identifier.indexOf(codeString[i])!=-1){if(i-2<codeString.length&&codeString.substr(i,2)=="<?"){insertRestCode();inTag=false;nowProcessingTagName=null;var processingCode;if(codeString.substr(i,5)=="<?php"){highlightedCode+=stylefy("keyword3","<?php");i+=4}else{highlightedCode+=stylefy("keyword3","<?");i+=1}var innerColorScripter=new ColorScripter("php-inner-code",this.stylePackage.packageName);innerColorScripter.setReturnMode(returnMode);innerColorScripter.convertingTabToSpace=this.convertingTabToSpace;var r=innerColorScripter.color(codeString.substr(i+1),"?>");if(typeof r=="string"){highlightedCode+=r;procEndOfHighlightedCode();return highlightedCode}else{highlightedCode+=r[0];i+=r[1];continue}}else if(i>0&&codeString[i]+codeString[i+1]=="?>"){insertRestCode();restCode="";highlightedCode+=stylefy("keyword3","?>");i++;continue}}}if(languageTypes.indexOf("asp")!=-1||languageTypes.indexOf("jsp")!=-1){if(this.identifier.indexOf(codeString[i])!=-1){if(i-2<codeString.length&&codeString.substr(i,2)=="<%"){insertRestCode();inTag=false;nowProcessingTagName=null;var processingCode;var innerLang=languageTypes.indexOf("asp")!=-1?"asp-inner-code":"jsp-inner-code";highlightedCode+=st